<?xml version="1.0" encoding="UTF-8"?>
<!-- Plus sign shaped 5x5 map with a few items scattered about.
     Same map as the one created by the default constructor for Map. -->
<map height="5" width="5">
	<location x="2" y="4">
		<item name="north_flag" />
	</location>
	<location x="2" y="3" />
	<location x="2" y="2" />
	<location x="0" y="2" />
	<location x="1" y="2" />
	<location x="3" y="2" />
	<location x="4" y="2" />
	<location x="2" y="1" />
	<location x="2" y="0">
		<item name="south_flag" />
		<item name="compass" />
	</location>
	<start x="2" y="2" />
	<inventory>
		<item name="transponder" />
	</inventory>
</map>
